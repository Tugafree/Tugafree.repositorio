<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.tugafree" name="Tugafree" version="0.0.2" provider-name="Korea, Vitorgalo, AndryC, Grisalho">
	<requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.beautifulsoup" version="3.2.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <language>Pt</language>
    <platform>all</platform>
    <summary lang="en">assista tv ao vivo</summary>
    <description lang="en">nao transmitimos nenhum stream,tudo o que esta aqui esta ao alcance de qualquer um na internet!</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="Tugafree.repositorio" name="Tugafree Repositorio" version="0.0.2" provider-name="Tugafree">
	<extension point="xbmc.addon.repository" name="Tugafree Add-on Repository">
		<info compressed="false">http://raw.github.com/Tugafree/Tugafree.repositorio/master/addons.xml</info>
		<checksum>http://raw.github.com/Tugafree/Tugafree.repositorio/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/Tugafree/Tugafree.repositorio/master/repo/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Tugafree Addon</summary>
		<description>Addon Description</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
